    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.28",
    "teenytest": "^6.0.2",
    "teenytest-promise": "^1.0.0",
    "testdouble": "^3.16.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
